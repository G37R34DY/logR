plot.normal <- function(mean, sd) {
xseq<-seq((mean - 6*sd),(mean +6*sd),1)
densities<-dnorm(xseq, mean,sd)
plot(xseq, densities, col="darkgreen",xlab="", ylab="Density", type="l",lwd=2, cex=2, main="PDF of Standard Normal", cex.axis=.8)
}
plot.normal(80,30)
plot.normal(80,30)
